//C++ program for recursively print 1 to n.
#include <bits/stdc++.h>
using namespace std;
//  This fun1 will print from n to 1.
void fun1(int n)
{
    if(n==0) return;
    cout<<n<<" ";
    fun1( n-1);
}

//fun2 will print from 1 to n.
void fun2(int n)
{
    if(n<=0)return;
    fun2(n-1);
    cout<<n<<" ";
}
int main()
{
    fun1(5);
    cout<<endl;
    fun2(5);
}
